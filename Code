def kangaroo(x1, v1, x2, v2):
    
    
    if v1>v2:
        
        a1=x1-x2
        a2=v2-v1
        d=abs(a1%a2)
        
        if d==0:
            return "YES"
        else:
            return "NO"
    else:
        return "NO"

x1,v1,x2,v2=map(int,input().split(" "))
print(kangaroo(x1,v1,x2,v2))
